.searchenter{
  display: flex;
height: 100rpx;
width: 100%;
border-bottom: 3rpx solid gray;
}
.searchenter .enter{
  flex-direction: row;
  border: 2rpx solid skyblue;
  width: 60%;
  height: 30rpx;
  margin-top: 25rpx;
  margin-left: 10rpx;
  border-radius: 10rpx;
}
.searchenter .looking{
  flex-direction: row;
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid rgba(7, 17, 27, 0.6);
  margin-top: 25rpx;
  margin-left: 10rpx;
  border-radius: 10rpx;
  background-color: orange
}
.searchenter .looking .img{
  width: 50rpx;
  height: 50rpx;
}
.searchenter .back{
  width: 100rpx;
  height: 50rpx;
  border: 1rpx solid rgba(7, 17, 27, 0.6);
  margin-top: 25rpx;
  margin-left: 20rpx;
  border-radius: 10rpx;
   background-color: skyblue
  
}
.searchenter .back span{
  font-size: 30rpx;
  color: rgba(7, 17, 27, 0.6);
  margin-left: 20rpx;
  margin-top: 15rpx;
 
}
.tag{
  font-size: 30rpx;
  color: rgba(7, 17, 27, 0.6)
}
.provincecity{
  width: 100%;
  height: 500rpx;
  border: 1rpx solid rgba(7, 17, 27, 0.6);
  border-radius: 10rpx;
  margin-top: 10rpx;
  display: flex;
}
.provincecity .city{
  width: 140rpx;
  height: 500rpx;
  
 flex-direction: row;
 margin-left: 6rpx;
}
.provincecity .city .btn{
  margin-top: 18rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border: 1rpx solid rgba(7, 17, 27, 0.6);
  color: rgba(7, 17, 27, 0.6)
}